<Window x:Class="Starship.Bot.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Starship.Bot.Windows"
        mc:Ignorable="d"
        Title="MainWindow" Width="800" Height="600" Loaded="Window_Loaded">
    <DockPanel Margin="5,5,5,5">
        <ListBox x:Name="RegionList" DockPanel.Dock="Left" Width="200" SelectionChanged="RegionList_SelectionChanged" />
        <TextBlock x:Name="FPS" DockPanel.Dock="Right" VerticalAlignment="Bottom" HorizontalAlignment="Right"></TextBlock>
    </DockPanel>
</Window>

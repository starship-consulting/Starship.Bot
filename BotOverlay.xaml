<Window x:Class="Starship.Bot.BotOverlay"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Starship.Bot"
        mc:Ignorable="d"
        Title="Overlay" AllowsTransparency="True" WindowStyle="None" Loaded="Window_Loaded" ShowInTaskbar="False" ResizeMode="NoResize" WindowStartupLocation="CenterOwner" MouseDown="Window_MouseDown" MouseMove="Window_MouseMove" MouseUp="Window_MouseUp">
    <Window.Background>
        <SolidColorBrush Opacity="0.01" Color="Black" x:Name="BackgroundColor" />
    </Window.Background>
    <Canvas Name="MainCanvas"></Canvas>
</Window>
